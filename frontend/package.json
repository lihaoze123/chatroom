  "scripts": {
    "build-css": "tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "build-css-once": "tailwindcss -i ./src/input.css -o ./src/output.css",
    "start": "npm run build-css-once && react-scripts start",
    "start:network": "npm run build-css-once && cross-env HOST=0.0.0.0 react-scripts start",
    "build": "tailwindcss -i ./src/input.css -o ./src/output.css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },

  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
